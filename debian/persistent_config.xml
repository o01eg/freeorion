<?xml version="1.0"?>
<XMLDoc>
 <save>
  <auto>
   <hostless>
    <enabled>1</enabled>
   </hostless>
   <exit>
    <enabled>1</enabled>
   </exit>
   <interval>7200</interval>
  </auto>
 </save>
 <setup>
  <rules>
   <RULE_ENABLE_SUPER_TESTER>0</RULE_ENABLE_SUPER_TESTER>
   <RULE_THRESHOLD_HUMAN_PLAYER_WIN>1</RULE_THRESHOLD_HUMAN_PLAYER_WIN>
   <RULE_ALLOW_CONCEDE>1</RULE_ALLOW_CONCEDE>
   <RULE_CONCEDE_COLONIES_THRESHOLD>9999</RULE_CONCEDE_COLONIES_THRESHOLD>
   <RULE_SHOW_DETAILED_EMPIRES_DATA>0</RULE_SHOW_DETAILED_EMPIRES_DATA>
   <server-locked>
    <RULE_ENABLE_SUPER_TESTER>1</RULE_ENABLE_SUPER_TESTER>
    <RULE_THRESHOLD_HUMAN_PLAYER_WIN>1</RULE_THRESHOLD_HUMAN_PLAYER_WIN>
    <RULE_ALLOW_CONCEDE>1</RULE_ALLOW_CONCEDE>
    <RULE_CONCEDE_COLONIES_THRESHOLD>1</RULE_CONCEDE_COLONIES_THRESHOLD>
    <RULE_SHOW_DETAILED_EMPIRES_DATA>1</RULE_SHOW_DETAILED_EMPIRES_DATA>    
   </server-locked>
  </rules>
 </setup>
 <network>
  <server>
   <ai>
    <max>5</max>
   </ai>
   <human>
    <min>2</min>
   </human>
   <conn-human-empire-players>
    <min>2</min>
   </conn-human-empire-players>
   <unconn-human-empire-players>
    <max>2</max>
   </unconn-human-empire-players>
   <publish-statistics>0</publish-statistics>
   <cookies>
    <expire-minutes>120</expire-minutes>
   </cookies>
   <binary>
    <enabled>0</enabled>
   </binary>
   <client-message-size>
     <max>10485760</max>
   </client-message-size>
  </server>
 </network>
</XMLDoc>
