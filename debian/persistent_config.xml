<?xml version="1.0"?>
<XMLDoc>
 <save>
  <auto>
   <hostless>
    <enabled>0</enabled> <!-- Enable -->
    <each-player>
     <enabled>0</enabled> <!-- Enable -->
    </each-player>
   </hostless>
   <exit>
    <enabled>1</enabled>
   </exit>
   <interval>21600</interval>
  </auto>
 </save>
 <setup>
  <ai>
   <player>
    <count>1</count> <!-- Disable -->
   </player>
  </ai>
  <monster>
    <frequency>MONSTER_SETUP_LOW</frequency>
  </monster>
  <native>
    <frequency>GALAXY_SETUP_MEDIUM</frequency>
  </native>
  <specials>
    <frequency>GALAXY_SETUP_MEDIUM</frequency>
  </specials>
  <planet>
    <density>GALAXY_SETUP_MEDIUM</density>
  </planet>
  <galaxy>
   <shape>BOX</shape>
   <age>GALAXY_SETUP_MEDIUM</age>
  </galaxy>
  <seed>RANDOM</seed>
  <star>
   <count>46</count>
  </star>
  <rules>
   <RULE_ENABLE_SUPER_TESTER>0</RULE_ENABLE_SUPER_TESTER>
   <RULE_ENABLE_EXPERIMENTORS>0</RULE_ENABLE_EXPERIMENTORS>
   <RULE_THRESHOLD_HUMAN_PLAYER_WIN>1</RULE_THRESHOLD_HUMAN_PLAYER_WIN>
