  </rules>
 </setup>
 <network>
  <server>
   <ai>
    <max>1</max> <!-- Disable -->
   </ai>
   <allow-email>
    <new-turn>0</new-turn> <!-- Enable -->
   </allow-email>
   <allow-observers>1</allow-observers> <!-- Disable -->
   <send-turn-chat>0</send-turn-chat> <!-- Enable -->
   <human>
    <min>3</min>
   </human>
   <conn-human-empire-players>
    <min>0</min>
   </conn-human-empire-players>
   <unconn-human-empire-players>
    <max>0</max>
   </unconn-human-empire-players>
   <publish-statistics>1</publish-statistics> <!-- Disable in real game -->
   <publish-seed>1</publish-seed> <!-- Disable in real game -->
   <drop-empire-ready>0</drop-empire-ready>
   <cookies>
    <expire-minutes>5760</expire-minutes>
   </cookies>
   <binary>
    <enabled>0</enabled>
   </binary>
   <turn-timeout>
     <fixed-interval>0</fixed-interval>
     <max-interval>300</max-interval> <!-- 48h to hours = 172800 -->
   </turn-timeout>
   <client-message-size>
     <max>9485760</max>
   </client-message-size>
  </server>
 </network>
 <logging>
  <sources>
   <network>debug</network>
  </sources>
 </logging>
</XMLDoc>
