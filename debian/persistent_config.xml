   <RULE_TECH_COST_FACTOR>1.2</RULE_TECH_COST_FACTOR>
   <!--RULE_DIPLOMACY>RULE_DIPLOMACY_FORBIDDEN_FOR_ALL</RULE_DIPLOMACY>-->
