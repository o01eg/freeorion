   <RULE_CONCEDE_COLONIES_THRESHOLD>9999</RULE_CONCEDE_COLONIES_THRESHOLD>
   <RULE_SHIP_PART_BASED_UPKEEP>1</RULE_SHIP_PART_BASED_UPKEEP>
   <RULE_SHIP_HULL_COST_FACTOR>0.6</RULE_SHIP_HULL_COST_FACTOR>
